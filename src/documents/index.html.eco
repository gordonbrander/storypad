---
title: 'Future themes'
layout: 'default'
---
<div class="in">
  <div class="unit6">
    <ul>
    <% for document in @getCollection('html').toJSON().filter(@negatedSearcher('url', 'index.html')).sort(@compareByFilename): %>
      <li><a href="<%= document.url %>"><%= document.title %></a></li>
    <% end %>
    </ul>
  </div>
</div>