---
title: 'Future themes'
layout: 'default'
---
<div class="artboard-main">
  <div class="in">
    <div class="unit6">
      <h1><%= @site.title %></h1>
      <ul>
      <% for document in @getHtmlJsonSortedByFilename(): %>
        <li><a href="<%= @prependBaseUrl(document.url) %>"><%= document.title %></a></li>
      <% end %>
      </ul>
    </div>
  </div>
</div>