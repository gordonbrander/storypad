---
layout: 'artboard'
---
<div class="in">
  <article class="article">
    <% if @document.images: %>
      <% for url in document.images: %>
        <img src="<%= @prependBaseUrl(url) %>" />
      <% end %>
    <% end %>
    <div class="content unit6">
      <%- @content %>
    </div>
  </article>
</div>
